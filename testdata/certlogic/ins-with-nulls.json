{
  "name": "ins with nulls",
  "cases": [
    {
      "name": "null-safety",
      "certLogicExpression": {
        "in": [
          {
            "var": "x"
          },
          {
            "var": "y"
          }
        ]
      },
      "assertions": [
        {
          "directive": "skip",
          "data": null,
          "expected": false
        },
        {
          "directive": "skip",
          "data": {},
          "expected": false
        },
        {
          "directive": "skip",
          "data": {
            "x": null
          },
          "expected": false
        },
        {
          "directive": "skip",
          "data": {
            "x": null,
            "y": null
          },
          "expected": false
        },
        {
          "data": {
            "x": null,
            "y": []
          },
          "expected": false
        },
        {
          "data": {
            "x": null,
            "y": [ null ]
          },
          "expected": true
        }
      ]
    },
    {
      "name": "in on a null doesn't eval to true",
      "certLogicExpression": {
        "in": [
          {
            "var": "x"
          },
          {
            "var": "y"
          }
        ]
      },
      "assertions": [
        {
          "data": {
            "x": null,
            "y": [
              "a",
              "b"
            ]
          },
          "expected": false
        }
      ]
    },
    {
      "name": "works as expected without nulls",
      "certLogicExpression": {
        "in": [
          {
            "var": "x"
          },
          {
            "var": "y"
          }
        ]
      },
      "assertions": [
        {
          "data": {
            "x": "a",
            "y": [
              "a",
              "b"
            ]
          },
          "expected": true
        },
        {
          "data": {
            "x": "c",
            "y": [
              "a",
              "b"
            ]
          },
          "expected": false
        }
      ]
    }
  ]
}
